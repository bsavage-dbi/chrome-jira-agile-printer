{
    "manifest_version": 2,
    "name": "Jira Print View",
    "description": "Jira Print View",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://*.atlassian.net/*"
            ],
            "js": [
                "index.min.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        //        "http://*/*",
        //        "https://*/*",
        //        "https://ajax.googleapis.com/"
    ],
    "web_accessible_resources": [
        "app.min.css",
        "templates.html",
        "print.min.js"
    ],
    "icons": {
//        "16": "icon16.png",
//        "48": "icon48.png",
        "128": "icon128.png"
    }
}